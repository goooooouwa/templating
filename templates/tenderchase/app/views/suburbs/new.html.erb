<% content_for :header do %>
  <%= render partial: "layouts/header", locals: {active_tab: Suburb} %>
<% end %>

<% content_for :navigation_title do %>
  <%= render partial: "layouts/navigation_title", locals: {title: t('.title', :default => [:'helpers.titles.new', 'New %{model}'], :model => Suburb.model_name.human.titleize)} %>
<% end %>

<% content_for :navigation_actions do %>
  <div class="btn-toolbar pull-right" role="toolbar">
    <div class="btn-group">
      <%= link_to t('.cancel', :default => t("helpers.links.cancel")), suburbs_path, :class => 'btn btn-default' %>
    </div>
  </div>
<% end %>

<% content_for :content do %>
  <%= render :partial => 'form' %>
<% end %>

<%= render template: "layouts/app" %>
