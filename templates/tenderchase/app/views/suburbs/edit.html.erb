<% content_for :header do %>
  <%= render partial: "layouts/header", locals: {active_tab: Suburb} %>
<% end %>

<% content_for :navigation_bar do %>
<div class="container-fluid">
  <div class="row">
    <div class="col-xs-4">
    </div>
    <div class="col-xs-4">
      <%= render partial: "layouts/navigation_title", locals: {title: t('.title', :default => [:'helpers.titles.edit', 'Edit %{model}'], :model => Suburb.model_name.human.titleize)} %>
    </div>
    <div class="col-xs-4">
      <div class="btn-toolbar pull-right" role="toolbar">
        <div class="btn-group">
          <%= link_to t('.cancel', :default => t("helpers.links.cancel")), @suburb, :class => 'btn btn-default' %>
        </div>
      </div>
    </div>
  </div>
</div>
<% end %>

<% content_for :content do %>
  <%= render :partial => 'form' %>
<% end %>

<%= render template: "layouts/app" %>
