<h1>Listing offices</h1>

<table>
  <thead>
    <tr>
      <th>Active</th>
      <th>Name</th>
      <th>Address line</th>
      <th>Suburb</th>
      <th>State</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Fax</th>
      <th>Notes</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @offices.each do |office| %>
      <tr>
        <td><%= office.active %></td>
        <td><%= office.name %></td>
        <td><%= office.address_line %></td>
        <td><%= office.suburb.name %></td>
        <td><%= office.state.name %></td>
        <td><%= office.latitude %></td>
        <td><%= office.longitude %></td>
        <td><%= office.email %></td>
        <td><%= office.phone %></td>
        <td><%= office.fax %></td>
        <td><%= office.notes %></td>
        <td><%= link_to 'Show', office %></td>
        <td><%= link_to 'Edit', edit_office_path(office) %></td>
        <td><%= link_to 'Destroy', office, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Office', new_office_path %>
